{
    "publisher": "EdgardMessias",
    "name": "override-mark",
    "displayName": "Override Mark",
    "description": "Show a gutter icon when has a implement/override method/property in javascript or typescript.",
    "version": "1.2.1",
    "icon": "resources/override-mark.png",
    "extensionKind": "ui",
    "engines": {
        "vscode": "^1.26.0"
    },
    "bugs": {
        "url": "https://github.com/edgardmessias/vscode.override-mark/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/edgardmessias/vscode.override-mark.git"
    },
    "homepage": "https://github.com/edgardmessias/vscode.override-mark",
    "categories": [
        "Other"
    ],
    "keywords": [
        "class",
        "decorator",
        "implement",
        "interface",
        "javascript",
        "jsx",
        "mark",
        "override",
        "tsx",
        "typescript"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:javascriptreact",
        "onLanguage:typescript",
        "onLanguage:typescriptreact"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [],
        "menus": {
            "commandPalette": [],
            "view/item/context": [],
            "view/title": []
        },
        "keybindings": [],
        "configuration": [
            {
                "title": "Override Mark",
                "properties": {
                    "override-mark.delay": {
                        "type": "integer",
                        "description": "The delay in milliseconds between when a change occurs and when a mark update is performed.",
                        "default": 500
                    }
                }
            }
        ]
    },
    "scripts": {
        "changelog:last": "conventional-changelog -p angular -r 2",
        "changelog:preview": "conventional-changelog -p angular -u",
        "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "clean": "rimraf ./out ./coverage ./test-reports",
        "compile": "tsc -p ./",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "organize": "node ./out/tools/organize.js",
        "release": "release-it",
        "test": "npm run compile && node ./out/test/runTests.js",
        "vscode:prepublish": "npm run clean && npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/istanbul-lib-coverage": "^2.0.3",
        "@types/istanbul-lib-hook": "^2.0.0",
        "@types/istanbul-lib-instrument": "^1.7.4",
        "@types/istanbul-lib-report": "^3.0.0",
        "@types/istanbul-lib-source-maps": "^4.0.1",
        "@types/istanbul-reports": "^3.0.0",
        "@types/mocha": "^8.0.3",
        "@types/node": "~8.10.59",
        "@types/sinon": "^9.0.8",
        "@types/vscode": "1.26.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "conventional-changelog-cli": "^2.1.1",
        "decache": "^4.6.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "glob": "^7.1.6",
        "istanbul-lib-coverage": "^3.0.0",
        "istanbul-lib-hook": "^3.0.0",
        "istanbul-lib-instrument": "^4.0.3",
        "istanbul-lib-report": "^3.0.0",
        "istanbul-lib-source-maps": "^4.0.0",
        "istanbul-reports": "^3.0.2",
        "mocha": "^8.1.3",
        "mocha-multi-reporters": "^1.5.1",
        "original-fs": "^1.1.0",
        "prettier": "^2.1.2",
        "release-it": "^14.2.1",
        "rimraf": "^3.0.2",
        "sinon": "^9.2.1",
        "source-map-support": "^0.5.19",
        "typescript": "^4.0.5",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {}
}
